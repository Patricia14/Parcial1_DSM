<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Salarios">

    <TextView
        android:id="@+id/DesRen1"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="50dp"
        app:layout_constraintStart_toEndOf="@+id/DesAfp1"
        app:layout_constraintTop_toBottomOf="@+id/empleado1" />

    <TextView
        android:id="@+id/DesRen2"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="50dp"
        app:layout_constraintStart_toEndOf="@+id/DesAfp1"
        app:layout_constraintTop_toBottomOf="@+id/SalEmp2" />

    <TextView
        android:id="@+id/DesRen3"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="50dp"
        app:layout_constraintStart_toEndOf="@+id/DesAfp3"
        app:layout_constraintTop_toBottomOf="@+id/SalEmp3" />

    <TextView
        android:id="@+id/empleado1"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/empleado2"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="140dp"
        android:layout_marginEnd="50dp"
        app:layout_constraintEnd_toStartOf="@+id/SalEmp2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/empleado1" />

    <TextView
        android:id="@+id/empleado3"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="140dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/empleado2" />

    <TextView
        android:id="@+id/NoBono"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="100dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/DesRen3" />

    <TextView
        android:id="@+id/SalEmp1"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toEndOf="@+id/empleado1"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/SalEmp2"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="140dp"
        app:layout_constraintStart_toEndOf="@+id/empleado2"
        app:layout_constraintTop_toBottomOf="@+id/SalEmp1" />

    <TextView
        android:id="@+id/SalEmp3"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="140dp"
        app:layout_constraintStart_toEndOf="@+id/empleado3"
        app:layout_constraintTop_toBottomOf="@+id/SalEmp2" />

    <TextView
        android:id="@+id/DesSeg1"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/empleado1" />

    <TextView
        android:id="@+id/DesAfp1"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="50dp"
        app:layout_constraintStart_toEndOf="@+id/DesSeg1"
        app:layout_constraintTop_toBottomOf="@+id/empleado1" />

    <TextView
        android:id="@+id/DesAfp2"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="50dp"
        app:layout_constraintStart_toEndOf="@+id/DesSeg2"
        app:layout_constraintTop_toBottomOf="@+id/empleado2" />

    <TextView
        android:id="@+id/DesAfp3"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="50dp"
        app:layout_constraintStart_toEndOf="@+id/DesSeg3"
        app:layout_constraintTop_toBottomOf="@+id/empleado3" />

    <TextView
        android:id="@+id/DesSeg2"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/empleado2" />

    <TextView
        android:id="@+id/DesSeg3"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/empleado3" />

    <Button
        android:id="@+id/btnRegresar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="159dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="159dp"
        android:onClick="Atras"
        android:text="Regresar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/NoBono" />
</androidx.constraintlayout.widget.ConstraintLayout>